{
  "expo": {
    "name": "EtiloMe",
    "slug": "EtiloMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ios_dark.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "icon": {
        "dark": "./assets/icons/ios_dark.png",
        "light": "./assets/icons/ios_light.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "monochromeImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/ios_light.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/icons/ios_dark.png",
            "resizeMode": "contain",
            "backgroundColor": "#343434"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
