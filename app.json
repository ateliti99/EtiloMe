{
  "expo": {
    "name": "EtiloMe",
    "slug": "EtiloMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ios_dark.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "icon": {
        "dark": "./assets/icons/ios_dark.png",
        "light": "./assets/icons/ios_light.png"
      },
      "bundleIdentifier": "com.ateliti99.EtiloMe",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "monochromeImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ateliti99.EtiloMe"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/ios_light.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/icons/ios_dark.png",
            "resizeMode": "contain",
            "backgroundColor": "#343434"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "iosAppId": "ca-app-pub-3383825260934343~6967885569",
          "androidAppId": "ca-app-pub-3383825260934343~9629669930",
          "userTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c240e4f6-b5a2-4a65-a7ad-41722e172ec8"
      }
    },
    "owner": "ateliti99"
  }
}
